<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>@ViewData["Title"] - PapugarniaOnline</title>
    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/css/site.css" />
</head>
<body>
    <header>
        <nav class="navbar navbar-expand-sm navbar-toggleable-sm navbar-light bg-white border-bottom box-shadow mb-3">
            <div class="container">
                <a class="navbar-brand" asp-area="" asp-controller="Home" asp-action="Index">Papugarnia Yellowstone</a>
                <button class="navbar-toggler" type="button" data-toggle="collapse" data-target=".navbar-collapse" aria-controls="navbarSupportedContent"
                        aria-expanded="false" aria-label="Toggle navigation">
                    <span class="navbar-toggler-icon"></span>
                </button>
                <div class="navbar-collapse collapse d-sm-inline-flex flex-sm-row-reverse">
                    <partial name="_LoginPartial" />
                    <ul class="navbar-nav flex-grow-1">
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Index">Home</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Parrots" asp-action="Index">Our Parrots</a>
                        </li>
                        <li class="nav-item">
                            <a class="nav-link text-dark" asp-area="" asp-controller="Tickets" asp-action="Index">Show Tickets</a>
                        </li>
                    </ul>
                </div>
            </div>
        </nav>
    </header>

    <img class="nature2" width="100%" height="600px" src="https://s3.eu-central-1.amazonaws.com/pressland-cms/cache/article_show_cover/s5/17963184-biala-kakadu-szybko-przywiazuje-sie-do-opiekuna-ale.jpeg" />
    <img class="nature2" width="100%" height="600px" src="https://kpck.pl/wp-content/uploads/2016/07/Kakadu-Papugi.jpg" />
    <img class="nature2" width="100%" height="600px" src="https://images6.alphacoders.com/774/774617.jpg" />
    <img class="nature2" width="100%" height="600px" src="https://images.alphacoders.com/449/thumb-1920-449417.jpg" />
    <img class="nature2" width="100%" height="600px" src="https://tapety.tja.pl/obrazki/tja_normalne/229918.jpg" />

    <div class="container">
        <main role="main" class="pb-3">
            @RenderBody()
        </main>
    </div>



    <footer class="border-top footer text-muted">
        <div class="container">
            &copy; 2020 - PapugarniaOnline - <a asp-area="" asp-controller="Home" asp-action="Privacy">Privacy</a>
        </div>
    </footer>
    <script src="~/lib/jquery/dist/jquery.min.js"></script>
    <script src="~/lib/bootstrap/dist/js/bootstrap.bundle.min.js"></script>
    <script src="~/js/site.js" asp-append-version="true"></script>
    @RenderSection("Scripts", required: false)
</body>
</html>


<script src="https://www.w3schools.com/lib/w3.js"></script>


<script>
    w3.slideshow(".nature2", 2500);
</script>